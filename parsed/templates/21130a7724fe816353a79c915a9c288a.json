{
  "aoc-custom/components/common/custom-navigation-dropdown": "<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\r\n   href=\"#\"\r\n   data-collapsible=\"navPages-{{id}}\"\r\n>Shop &nbsp; &nbsp;\r\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n    </i>\r\n</a>\r\n<div class=\"column-menu navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        {{#each categories}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                      \r\n                       <a class=\"navPage-subMenu-action dropdown-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                            <img class=\"mt-2 mb-2 nav-img\" src=\"{{cdn (concat '..img/nav/' (concat  (lowercase (dashcase name)) '.png'))}}\"  />\r\n                            <p class=\"categ-name\">{{name}}</p>\r\n                        </a>\r\n                </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n\r\n"
}
