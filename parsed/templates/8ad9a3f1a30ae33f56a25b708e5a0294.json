{
  "components/common/footer": "{{#if banners.bottom}}\r\n    {{#if settings.data_tag_enabled}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            {{#each (limit banners.bottom_metadata 1)}}\r\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                    <div data-event-type=\"promotion-click\">\r\n                        {{{this.content}}}\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            <div class=\"banner\">\r\n                {{{limit banners.bottom 1}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n\r\n\r\n                    \r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\r\n    <div class=\"container\">\r\n        {{#if settings.show_newsletter_box}}\r\n            <div class=\"footer-newsletter\" >\r\n                <div class=\"footer-newsletter-content\" >\r\n                    {{> components/common/subscription-form}}\r\n                </div>\r\n            </div>\r\n        {{/if}}\r\n\r\n        <section class=\"footer-info 4a616d6573 footer-custom\">\r\n            <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">\r\n                    <!-- {{lang 'footer.categories'}} -->\r\n                    Shop\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg><use xlink:href=\"#icon-downselector\" /></svg>\r\n                    </i>\r\n                </h5>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each categories}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    {{#if settings.gift_certificates_enabled}}\r\n                        <li>\r\n                            <a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n                        </li>\r\n                    {{/if}}\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">\r\n                    {{lang 'footer.navigate'}}\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg><use xlink:href=\"#icon-downselector\" /></svg>\r\n                    </i>\r\n                </h5>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each pages}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    <li>\r\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n        \r\n            <!-- {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\r\n                <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-brands\">\r\n                    <h5 class=\"footer-info-heading\">\r\n                        {{lang 'footer.brands'}}\r\n                        <i class=\"icon\" aria-hidden=\"true\">\r\n                            <svg><use xlink:href=\"#icon-downselector\" /></svg>\r\n                        </i>\r\n                    </h5>\r\n                    <ul class=\"footer-info-list\">\r\n                        {{#each shop_by_brand}}\r\n                            <li>\r\n                                <a href=\"{{url}}\">{{name}}</a>\r\n                            </li>\r\n                        {{/each}}\r\n                        <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n                    </ul>\r\n                </article>\r\n            {{/and}} -->\r\n            \r\n                <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">Help</h5>\r\n                    <ul class=\"footer-info-list\">\r\n                            <li> <a href=\"#\">Returns and Exchanges</a></li>\r\n                            <li><a href=\"#\">Contact Us</a></li>\r\n                    </ul>\r\n                </article>\r\n                <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">Account</h5>\r\n                    <ul class=\"footer-info-list\">\r\n                            <li> <a class=\"navUser-action\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">Sign In</a></li>\r\n                            <li><a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">Create Account</a></li>\r\n                    </ul>\r\n                </article>\r\n\r\n            \r\n            \r\n            {{#if theme_settings.social_icons_footer}}\r\n                <article class=\"footer-info-col footer-mobile-limited footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n                    {{> components/common/social-links}}\r\n                </article>\r\n            {{/if}}\r\n            \r\n            <!-- {{#if settings.address}}\r\n                <article class=\"footer-info-col footer-mobile-limited footer-info-col--small\" data-section-type=\"storeInfo\">\r\n                    <h5 class=\"footer-info-heading footer-store-name\">{{settings.store_name}}</h5>\r\n                    <address>{{nl2br settings.address}}</address>\r\n                    {{#if settings.phone_number}}\r\n                        <div class=\"footer-info-phone\">\r\n                            <i class=\"icon\" aria-hidden=\"true\">\r\n                                <svg><use xlink:href=\"#icon-phone\" /></svg>\r\n                            </i>\r\n                            <a href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a>\r\n                        </div>\r\n                    {{/if}}\r\n                </article>\r\n            {{/if}} -->\r\n        </section>\r\n        <div class=\"footer-copyright\"  style=\"display: none;\">\r\n            {{> components/common/payment-icons}}\r\n            {{#if theme_settings.show_copyright_footer}}\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}. {{lang 'footer.right_reserved'}} </p>\r\n            {{/if}}\r\n        </div>\r\n        {{#or theme_settings.show_powered_by theme_settings.show_built_by}}\r\n            <div class=\"footer-copyright\" style=\"font-size: 12px;\">\r\n                {{#if theme_settings.show_powered_by}}\r\n                    <p class=\"powered-by\" style=\"display: none;\">Powered by <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\r\n                {{/if}}\r\n                <ul class=\"navPages-list footer-list\">\r\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"#\" >Terms & Conditions</a></li>\r\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"#\">&nbsp; Privacy Policy</a></li>\r\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"#\">&nbsp; FDA Statement</a></li>\r\n                </ul>\r\n                \r\n                \r\n                {{#if theme_settings.show_built_by}}\r\n                    <p class=\"powered-by\" style=\"display: none;\">Theme by <a href=\"https://www.weizenyoung.com/\" rel=\"nofollow\">Weizen Young</a></p>\r\n                \r\n                {{/if}}\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}. {{lang 'footer.right_reserved'}} </p>\r\n            </div>\r\n        {{/or}}\r\n    </div>\r\n    {{{region name=\"ssl_site_seal--global\"}}}\r\n</footer>\r\n",
  "components/common/subscription-form": "<div class=\"subs-container\">\r\n    <div class=\"subs-con\" >\r\n        <img src=\"{{cdn '..img/homepage/footer-envelope.png'}}\">\r\n    </div>\r\n    <div class=\"subs-con-1\" >\r\n        <h5>{{lang 'newsletter.subscribe'}}</h5>\r\n        <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do.</p>\r\n    </div>\r\n</div>\r\n{{#if settings.show_newsletter_summary }}\r\n    <div class=\"footer-newsletter-summary\"><p>{{settings.newsletter_summary}}</p>\r\n        </div>\r\n{{/if}}\r\n\r\n<div class=\"form-container subs-con-2\">\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <i class=\"icon\" aria-hidden=\"true\" style=\"display: none;\">\r\n                    <svg><use xlink:href=\"#icon-mail\" /></svg>\r\n                </i>\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"{{customer.email}}\"\r\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                >\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\r\n                       type=\"submit\"\r\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\r\n                >\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n</div>",
  "components/common/social-links": "{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\r\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg> \r\n                </a>\r\n               <h6>{{display_name}} </h6>\r\n            </li>\r\n        {{/each}}\r\n            \r\n    </ul>\r\n{{/if}}\r\n",
  "components/common/payment-icons": "{{#with theme_settings}}\r\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\r\n    <div class=\"footer-payment-icons\">\r\n        {{#if show_accept_amex}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_discover}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_mastercard}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_paypal}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_visa}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_amazonpay}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_googlepay}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_applepay}}\r\n        <svg class=\"footer-payment-icon footer-payment-icon-apple\"><use xlink:href=\"#icon-logo-apple-pay\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_klarna}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-klarna\"></use></svg>\r\n        {{/if}}\r\n    </div>\r\n    {{/or}}\r\n{{/with}}\r\n"
}
