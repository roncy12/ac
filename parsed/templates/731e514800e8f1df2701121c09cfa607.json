{
  "components/blog/post": "<article class=\"blog{{#if post.body}} full-post{{/if}}\">\r\n    <div class=\"blog-post-figure\">\r\n        {{#if post.thumbnail}}\r\n            <figure class=\"blog-thumbnail\">\r\n                <a href=\"{{post.url}}\">\r\n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\r\n                </a>\r\n            </figure>\r\n        {{/if}}\r\n    </div>\r\n\r\n    <div class=\"blog-post-body\">\r\n        <header class=\"blog-header\">\r\n            <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\r\n            <h3 class=\"blog-title\">\r\n                <a href=\"{{post.url}}\">{{post.title}}</a>\r\n            </h3>\r\n        </header>\r\n\r\n        <div class=\"blog-post\">\r\n            {{#if post.body}}\r\n                {{{post.body}}}\r\n            {{else}}\r\n                {{{post.summary}}}\r\n                {{#if post.show_read_more}}\r\n                    &hellip; <p class=\"read-more\"><a href=\"{{url}}\" aria-label=\"{{post.title}} {{lang 'blog.read_more'}}\"><strong>{{lang 'blog.read_more'}}</strong>\r\n                        <i class=\"icon\" aria-hidden=\"true\">\r\n                            <svg><use xlink:href=\"#icon-chevron-right\" /></svg>\r\n                        </i>\r\n                    </a></p>\r\n                {{/if}}\r\n            {{/if}}\r\n        </div>\r\n\r\n\r\n        {{#if show_tags}}\r\n            {{#if post.tags}}\r\n                <ul class=\"tags\">\r\n                    {{#each post.tags}}\r\n                    <li class=\"tag\">\r\n                        <a href=\"{{url}}\">#{{name}}</a>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n            {{/if}}\r\n        {{/if}}\r\n\r\n        {{>components/common/share url=post.url}}\r\n\r\n    </div>\r\n</article>\r\n",
  "components/common/share": "{{#if settings.add_this.buttons}}\r\n    {{assignVar 'encodedUrl' (encodeURI url)}}\r\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\r\n    <div>\r\n        <ul class=\"socialLinks\">\r\n            {{#each settings.add_this.buttons}}\r\n                <li class=\"socialLinks-item socialLinks-item--{{service}}\">\r\n                    {{#if service '===' 'facebook'}}\r\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\r\n                       title=\"{{{capitalize service}}}\"\r\n                       href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\r\n                       target=\"_blank\"\r\n                       rel=\"noopener\"\r\n                       {{{annotation}}}\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-facebook\"/>\r\n                        </svg>\r\n                    </a>\r\n                    {{else if service '===' 'email'}}\r\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\r\n                       title=\"{{{capitalize service}}}\"\r\n                       href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\r\n                       target=\"_self\"\r\n                       rel=\"noopener\"\r\n                       {{{annotation}}}\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-envelope\"/>\r\n                        </svg>\r\n                    </a>\r\n                    {{else if service '===' 'print'}}\r\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\r\n                       title=\"{{{capitalize service}}}\"\r\n                       onclick=\"window.print();return false;\"\r\n                       {{{annotation}}}\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-print\"/>\r\n                        </svg>\r\n                    </a>\r\n                    {{else if service '===' 'twitter'}}\r\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\r\n                       href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\r\n                       target=\"_blank\"\r\n                       rel=\"noopener\"\r\n                       title=\"{{{capitalize service}}}\"\r\n                       {{{annotation}}}\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-twitter\"/>\r\n                        </svg>\r\n                    </a>\r\n                    {{else if service '===' 'linkedin'}}\r\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\r\n                       title=\"{{{capitalize service}}}\"\r\n                       href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\r\n                       target=\"_blank\"\r\n                       rel=\"noopener\"\r\n                       {{{annotation}}}\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-linkedin\"/>\r\n                        </svg>\r\n                    </a>\r\n                    {{else if service '===' 'pinterest'}}\r\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\r\n                       title=\"{{{capitalize service}}}\"\r\n                       href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;description={{getVar 'encodedTitle'}}\"\r\n                       target=\"_blank\"\r\n                       rel=\"noopener\"\r\n                       {{{annotation}}}\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-pinterest\"/>\r\n                        </svg>\r\n                    </a>\r\n                    {{/if}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n{{/if}}\r\n{{#if settings.facebook_like_button.enabled}}\r\n    <iframe\r\n        class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\r\n        style=\"border:none; overflow:hidden; margin: 0; padding: 0; position:absolute\"\r\n        src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\r\n        scrolling=\"no\"\r\n        frameborder=\"0\"\r\n        allowTransparency=\"true\">\r\n    </iframe>\r\n{{/if}}\r\n"
}
